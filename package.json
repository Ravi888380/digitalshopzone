    "express-async-handler": "^1.1.4",
    "git": "^0.1.5",
    "jsonwebtoken": "^8.5.1",
    "module": "^1.2.5",
    "modules": "^0.4.0",
    "mongoose": "^5.10.9",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "react-social-login": "^3.4.10",
    "react-social-login-buttons": "^3.1.0"
  },
